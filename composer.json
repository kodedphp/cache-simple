{
  "name": "koded/cache-simple",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "A simple caching library for PHP 7 using several caching technologies.",
  "keywords": ["caching", "cache", "memcached", "redis", "file-cache", "psr-16", "php7"],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "http://kodeart.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "koded/stdlib": "~1",
    "psr/simple-cache": "~1"
  },
  "autoload": {
    "psr-4": {
      "Koded\\Caching\\": ""
    },
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "mikey179/vfsStream": "*",
    "satooshi/php-coveralls": "~1",
    "predis/predis": "dev-master"
  }
}
