{
  "name": "koded/cache-simple",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "A simple caching library for PHP 7 using several caching technologies. Supports JSON data caching for Redis.",
  "keywords": [
    "caching",
    "cache",
    "memcached",
    "redis",
    "json",
    "file-cache",
    "psr-16",
    "php7"
  ],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "http://kodeart.com"
    }
  ],
  "require": {
    "php": "^7.1.4",
    "koded/stdlib": "dev-4.0-dev",
    "psr/simple-cache": "~1",
    "psr/log": "~1",
    "ext-json": "*"
  },
  "suggest": {
    "ext-memcached": "For caching in Memcached",
    "ext-redis": "For caching in Redis",
    "ext-igbinary": "For Redis igbinary support",
    "ext-msgpack": "For de/serializing the cache data",
    "predis/predis": "If you want to use Redis without ext-redis extension"
  },
  "autoload": {
    "psr-4": {
      "Koded\\Caching\\": ""
    },
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "mikey179/vfsStream": "~1",
    "predis/predis": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
